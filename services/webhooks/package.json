{
  "name": "webhooks",
  "version": "1.0.0",
  "description": "Standalone version of elastic.io platform",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js src spec",
    "pretest": "npm run lint",
    "test": "mocha spec --recursive",
