  "license": "APL-2.0",
  "dependencies": {
    "@openintegrationhub/event-bus": "*",
    "@openintegrationhub/webhooks": "*",
    "awilix": "4.2.0",
    "backend-commons-lib": "*",
    "body-parser": "1.18.3",
    "express": "4.16.3",
    "mongoose": "5.4.11"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "4.5.0",
    "eslint-plugin-mocha": "4.12.1",
    "eslint-plugin-node": "6.0.1",
    "mocha": "6.0.0",
    "sinon": "7.2.4",
    "sinon-chai": "3.3.0"
  },
  "engines": {
    "node": ">=8"
  }
