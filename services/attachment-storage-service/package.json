{
    "name": "attachment-storage",
    "version": "0.0.1",
    "description": "Service for storing binary data",
    "main": "dist/index.js",
    "directories": {
        "doc": "docs"
    },
    "engines": {
        "node": ">= 10"
    },
    "scripts": {
        "start": "node ./dist/app.js",
        "dev": "NODE_ENV=dev ts-node-dev ./src/app.ts | bunyan",
        "dev:log_debug": "LOG_LEVEL=debug npm run dev",
        "test": "NODE_ENV=test node --max-old-space-size=3071 node_modules/.bin/_mocha --exit --forbid-only --require ts-node/register --require mocha-steps 'src/**/*.spec.ts'",
        "test:watch": "npm run test -- --watch-extensions 'ts,js,json' -w || reset",
        "eslint": "eslint -c ./.eslintrc.js --no-eslintrc --ext .ts .",
        "build": "tsc",
        "pretest": "npm run eslint"
    },
    "license": "UNLICENSED",
    "priavte": true,
    "devDependencies": {
        "@types/bunyan": "1.8.5",
        "@types/chai": "4.1.7",
        "@types/ioredis": "4.0.10",
        "@types/jsonwebtoken": "8.3.2",
        "@types/koa": "2.0.48",
        "@types/koa-bunyan-logger": "2.1.0",
        "@types/koa-jwt": "3.3.0",
        "@types/koa-router": "7.0.40",
        "@types/mocha": "5.2.6",
        "@types/mocha-steps": "1.1.1",
        "@types/nconf": "0.10.0",
        "@types/node": "12.0.2",
        "@types/supertest": "2.0.7",
        "@types/uuid": "3.4.4",
        "@types/uuid-validate": "0.0.1",
        "@typescript-eslint/eslint-plugin": "1.9.0",
        "@typescript-eslint/parser": "1.9.0",
        "chai": "4.2.0",
        "eslint": "5.16.0",
        "js-yaml": "3.13.1",
        "jsonwebtoken": "8.5.1",
        "mocha": "6.1.4",
        "mocha-steps": "1.3.0",
        "supertest": "4.0.2",
        "ts-node": "8.0.2",
        "ts-node-dev": "1.0.0-pre.32",
        "typescript": "3.3.3333",
        "uuid": "3.3.2"
    },
    "dependencies": {
        "@elastic.io/bunyan-logger": "1.0.4",
        "@openintegrationhub/attachment-storage-service": "*",
        "ioredis": "4.9.0",
        "koa": "2.7.0",
        "koa-bunyan-logger": "2.1.0",
        "koa-jwt": "3.5.1",
        "koa-router": "7.4.0",
        "nconf": "0.10.0",
        "uuid-validate": "0.0.3"
    }
}
